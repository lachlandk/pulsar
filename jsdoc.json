{
	"plugins": [
		"plugins/markdown"
	],
	"source": {
		"include": ["./build/web/pulsar-web.js"]
	},
	"opts": {
		"destination": "./docs/",
		"template": "./resources/default"
	},
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": ["jsdoc"]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"outputSourceFiles": false,
			"includeDate": false,
			"useLongnameInNav": true,
			"staticFiles": {
				"include": ["./resources/"]
			}
		}
	}
}

